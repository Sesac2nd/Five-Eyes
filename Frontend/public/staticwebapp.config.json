{
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS", "HEAD", "PATCH"],
      "rewrite": "https://5teamback.azurewebsites.net/api/{*restOfPath}",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS, PATCH",
        "Access-Control-Allow-Headers": "Content-Type, Authorization, X-Requested-With"
      }
    },
    {
      "route": "/health",
      "methods": ["GET", "HEAD"],
      "rewrite": "https://5teamback.azurewebsites.net/health"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/api/*",
      "*.{css,scss,sass,js,ts,jsx,tsx,mjs}",
      "*.{jpg,jpeg,png,gif,bmp,svg,ico,webp}",
      "*.{woff,woff2,ttf,eot}",
      "*.{json,xml,txt,map}"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css"
  }
}
